	<queryString>
		<![CDATA[select 	c.cpf, c.nome, f.funome, a.id as idaluguel, a.dtaluguel,
		a.dtdevolucao, i.tempo, i.quantidade, p.nome as pronome, s.descricao, count(m.mulid) as qtdemultas

from Cliente as c, Funcionario as f, Aluguel as a, itemaluguel as i, Produto as p, Status as s, Multa as m

where 	a.cliente_id = c.id
		and a.funcionario_funid = f.funid
        and i.status_id = s.id
        and i.aluguel_id = a.id
        and i.produto_id = p.id
        and m.mulaluid = a.id
        and i.id = $P{idaluguel}]]>
	</queryString>
